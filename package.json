{
  "name": "debt-destroyer",
  "version": "1.0.0",
  "description": "",
  "main": "src/js/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prepare-dirs": "mkdir dist",
    "build": "NODE_ENV=production && npm run build-primer && concurrently \"npm run build-core\" \"npm run build-extra-resources\"",
    "build-primer": "npm run clean && npm run prepare-dirs",
    "build-core": "concurrently \"npm run build-js\" \"npm run build-html\" \"npm run build-styles\"",
    "build-extra-resources": "concurrently \"npm run build-images\"",
    "build-js": "webpack -p --config webpack.config.js --color --display-error-details --devtool=sourcemap",
    "build-styles": "node-sass ./src/sass --output ./dist",
    "build-images": "cp -a ./src/img ./dist",
    "build-html": "cp -a ./src/html/ dist",
    "start": "npm run build && npm run watch-js & npm run watch-styles & npm run watch-images & npm run watch-html",
    "watch-js": "webpack --config webpack.config.js --progress --color --display-error-details --watch --debug --output-pathinfo",
    "watch-styles": "node-sass ./src/sass --output ./dist --watch",
    "watch-images": "cpx \"src/img/*\" dist/img --watch -v",
    "watch-html": "cpx \"src/html/**/*\" dist --watch -v"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-jsx": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2017": "^6.9.2",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "concurrently": "^2.2.0",
    "cpx": "^1.3.2",
    "grassy": "^0.1.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.1",
    "node-sass": "^3.7.0",
    "rimraf": "^2.5.2",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^2.1.0-beta.20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://3stacks@github.com/3stacks/deloitte.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/3stacks/deloitte/issues"
  },
  "homepage": "https://github.com/3stacks/deloitte#readme",
  "dependencies": {
    "chart.js": "^2.5.0",
    "moment": "^2.17.1",
    "vue": "^2.2.3"
  }
}
